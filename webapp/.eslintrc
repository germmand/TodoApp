{
    "extends": [
        "airbnb",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js"]}]
    }
}
